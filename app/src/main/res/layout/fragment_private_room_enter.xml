<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".PrivateRoomEnter">

  <TextView
    android:id="@+id/textView8"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="ENTER PRIVATE ROOM TOKEN"
    android:textColor="@color/black"
    android:textSize="23sp"
    android:layout_marginTop="200dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <EditText
    android:id="@+id/privateroomTokenEdittext"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="50dp"
    android:textSize="23sp"
    android:hint="Type..."
    app:layout_constraintTop_toBottomOf="@+id/textView8" />

  <Button
    android:id="@+id/goPrivate"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="GO TO PRIVATE ROOM"
    android:layout_marginTop="30dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/privateroomTokenEdittext" />
</androidx.constraintlayout.widget.ConstraintLayout>